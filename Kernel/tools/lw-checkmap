:
# Simple check of map file generated by lwlink (LWTOOLS)
grep ^Section "$1" | sort -k5 | awk '
  BEGIN{ errs=0 }

  {
	st=strtonum("0x"$6);
	if (en<st) {printf "free 0x%x-0x%x = %i\n",en,st,st-en};
	if (en>st) {printf "overlap at 0x%x-0x%x!\n",st,en; errs++};
	en=st+strtonum("0x"$8);
  }

  END{ if (errs>0) {exit 1}; }
'
